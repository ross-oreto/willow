{
    "name": "oreto/willow",
    "description": "willow-web",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "bcosca/fatfree-core": "3.7.3",
        "monolog/monolog": "^2.3",
        "oreto/f3-willow": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Oreto\\Willow\\": "src/",
            "Oreto\\F3Willow\\": "vendor/oreto/f3-willow/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oreto\\Willow\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ross-oreto"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/process": "^5.3"
    },
    "scripts": {
        "test": "cd tests && ../vendor/bin/phpunit --testdox ."
    },
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "oreto/f3-willow",
                "version":"master",
                "type": "library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ross-oreto/f3-willow.git",
                    "reference":"master"
                }
            }
        }
    ]
}
